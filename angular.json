{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mi-portfolio": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/mi-portfolio",
            "index": "src/index.html",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets",
              { "glob": "**/*", "input": "public" }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "outputHashing": "all",
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "sourceMap": true,
              "optimization": false
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "buildTarget": "mi-portfolio:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "mi-portfolio:build:production"
            },
            "development": {
              "buildTarget": "mi-portfolio:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
